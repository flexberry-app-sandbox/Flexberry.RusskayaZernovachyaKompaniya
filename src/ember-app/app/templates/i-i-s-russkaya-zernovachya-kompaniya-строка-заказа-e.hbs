<h3 class="ui header">{{t "forms.i-i-s-russkaya-zernovachya-kompaniya-строка-заказа-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.ценаСНалог
      class=(if (v-get validationModel "ценаСНалог" "isInvalid") "error")
      label=(t "forms.i-i-s-russkaya-zernovachya-kompaniya-строка-заказа-e.ценаСНалог-caption")
      data-test-i-i-s-russkaya-zernovachya-kompaniya-строка-заказа-e-ценаСНалог=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "ценаСНалог" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.количество
      class=(if (v-get validationModel "количество" "isInvalid") "error")
      label=(t "forms.i-i-s-russkaya-zernovachya-kompaniya-строка-заказа-e.количество-caption")
      data-test-i-i-s-russkaya-zernovachya-kompaniya-строка-заказа-e-количество=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "количество" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.сумма
      class=(if (v-get validationModel "сумма" "isInvalid") "error")
      label=(t "forms.i-i-s-russkaya-zernovachya-kompaniya-строка-заказа-e.сумма-caption")
      data-test-i-i-s-russkaya-zernovachya-kompaniya-строка-заказа-e-сумма=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "сумма" "message")}}
  </div>
</form>
